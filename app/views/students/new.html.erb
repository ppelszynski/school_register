<h1>Sign up as student</h1>

<%= simple_form_for @form, url: students_path do |f| %>
  <%= f.input :email %>

  <%= f.input :first_name %>

  <%= f.input :last_name %>

  <%= f.input :phone_number %>

  <%= f.input :password, autofocus: true, class: 'form-control'  %>

  <%= f.input :password_confirmation, autocomplete: 'off', class: 'form-control'  %>

  <%= f.button :submit, 'Sign as student', class: 'btn btn-primary' %>
<% end %>
